<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MAC\Medida" table="Medida">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="especura" type="float" column="especura" precision="10" scale="0" nullable="true"/>
    <field name="altura" type="float" column="altura" precision="10" scale="0" nullable="true"/>
    <field name="comprimento" type="float" column="comprimento" precision="10" scale="0" nullable="true"/>
    <field name="largura" type="float" column="largura" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
