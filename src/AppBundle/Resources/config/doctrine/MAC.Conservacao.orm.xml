<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MAC\Conservacao" table="Conservacao">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="temperaturamin" type="integer" column="temperaturaMin" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="temperaturamax" type="integer" column="temperaturaMax" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sensibilidadeluminosa" type="integer" column="sensibilidadeLuminosa" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
