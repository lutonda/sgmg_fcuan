<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Funcionario" table="Funcionario">
    <id name="idpessoa" type="integer" column="idPessoa">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="integer" column="tipo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="data" type="integer" column="data" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="exLogica" type="integer" column="exLogica" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
