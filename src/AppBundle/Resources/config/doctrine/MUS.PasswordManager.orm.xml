<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MUS\PasswordManager" table="PasswordManager">
    <indexes>
      <index name="idUtilizador" columns="idutilizador"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="validade" type="date" column="validade" nullable="true"/>
    <field name="data" type="datetime" column="data" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="valor" type="string" column="valor" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="estado" type="integer" column="estado" nullable="true">
    <options>
      <option name="fixed"/>
    </options>
  </field>
    <field name="urlValidator" type="string" column="urlValidator" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idUtilizador" target-entity="Usuario" fetch="LAZY">
      <join-columns>
        <join-column name="idUtilizador" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
