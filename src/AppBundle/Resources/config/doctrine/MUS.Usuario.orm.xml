<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MUS\Usuario" table="Usuario">
    <id name="id" type="integer" column="id">

    </id>
    <field name="username" type="string" column="username" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="integer" column="tipo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="roles" type="string" column="roles" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isactive" type="integer" column="isActive" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="exLogica" type="integer" column="exLogica" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
