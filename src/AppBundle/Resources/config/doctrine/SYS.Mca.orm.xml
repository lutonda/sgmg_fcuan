<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SYS\Mca" table="Mca">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="objecto" type="string" column="objecto" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descricao" type="string" column="descricao" length="100" nullable="true">
    <options>
      <option name="fixed"/>
    </options>
  </field>
    <field name="admin" type="integer" column="admin" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="gestor" type="integer" column="gestor" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="funcionario" type="integer" column="funcionario" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="visitante" type="integer" column="visitante" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="anonymous" type="integer" column="anonymous" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
